create database hcl;
use hcl
create table users(username varchar(50) primary key not null,password varchar(100),enabled int default 1);
insert into users values('hans','hcl',1);
insert into users values('madhu','hans',1);
insert into users values('user','pass',1);
create table user_roles(username varchar(50) primary key, role varchar(10), foreign key (username) references users(username));
insert into user_roles values('hans','ROLE_USER');
insert into user_roles values('madhu','ROLE_USER');
insert into user_roles values('user','ROLE_ADMIN');

